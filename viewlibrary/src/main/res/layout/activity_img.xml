<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
    android:background="#f0fae88e">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:orientation="horizontal">
        <walke.widget.img.RoundImageView2
            android:layout_width="110dp"
            android:layout_height="match_parent"
            android:src="@mipmap/meizi"
            android:scaleType="fitXY"
            android:background="#a670baff"
            app:border_Width="5dp"
            app:imgType="radius"
            app:borderColor="#fd1818"
            app:borderRadius="5"/>
        <walke.widget.img.RoundImageView2
            android:layout_width="110dp"
            android:layout_height="match_parent"
            android:src="@mipmap/meizi"
            android:scaleType="fitCenter"
            android:background="#a670baff"
            app:border_Width="5dp"
            app:imgType="radius"
            app:borderColor="#fd1818"
            app:borderRadius="5"
            android:layout_marginLeft="4dp"/>
        <FrameLayout
            android:layout_width="110dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp">
            <walke.widget.img.RoundImageView4
                android:layout_width="110dp"
                android:layout_height="match_parent"
                android:src="@mipmap/meizi"
                android:scaleType="fitCenter"
                android:background="#a670baff"
                app:border_Width="12dp"
                app:imgType="radius"
                app:borderColor="#fd1818"
                app:borderRadius="36"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg_corners"/>
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="horizontal"
        android:layout_marginTop="4dp">
        <walke.widget.img.RoundImageView3
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:src="@mipmap/meizi"
            android:scaleType="fitXY"
            android:background="#a670baff"
            app:imgType="circle"/>
        <walke.widget.img.RoundImageView4
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@mipmap/meizi"
            android:scaleType="fitCenter"
            android:background="#a670baff"
            app:border_Width="2dp"
            app:imgType="circle"
            app:borderColor="#18fd3a"
            android:layout_marginLeft="4dp"/>
    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="horizontal"
        android:layout_marginTop="4dp">
        <walke.widget.img.RoundImageView5
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:src="@mipmap/meizi"
            android:scaleType="fitCenter"
            android:background="#a670baff"
            app:border_Width="3dp"
            app:imgType="radius"
            app:borderColor="#fd1818"
            app:borderRadius="15"
            android:layout_marginLeft="4dp"/>
        <walke.widget.img.RoundImageView5
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@mipmap/meizi"
            android:scaleType="fitCenter"
            android:background="#a670baff"
            app:border_Width="2dp"
            app:imgType="circle"
            app:borderColor="#18fd3a"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:layout_marginTop="4dp">
        <walke.widget.img.RoundImageView6
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:src="@mipmap/meizi"
            android:scaleType="fitCenter"
            android:background="#a670baff"
            app:border_Width="3dp"
            app:imgType="radius"
            app:borderColor="#fd1818"
            app:borderRadius="15"
            android:layout_marginLeft="4dp"/>
        <walke.widget.img.RoundImageView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:src="@mipmap/meizi"
            android:scaleType="fitCenter"
            android:background="#a670baff"
            app:border_Width="3dp"
            app:imgType="radius"
            app:borderColor="#fd1818"
            app:borderRadius="15"
            android:layout_marginLeft="4dp"/>
        <walke.widget.img.RoundImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@mipmap/meizi"
            android:scaleType="fitCenter"
            android:background="#a670baff"
            app:border_Width="2dp"
            app:imgType="circle"
            app:borderColor="#f5f5f5"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"/>
    </LinearLayout>




</LinearLayout>